{
  "name": "make-it-heavy-typescript",
  "version": "1.0.0",
  "description": "TypeScript port of the Make It Heavy multi-agent system",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:heavy": "node dist/make-it-heavy.js",
    "dev": "ts-node src/main.ts",
    "dev:heavy": "ts-node src/make-it-heavy.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "mathjs": "^14.8.1",
    "openai": "^5.23.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
